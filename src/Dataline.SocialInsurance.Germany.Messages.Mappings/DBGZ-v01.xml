<?xml version="1.0" encoding="utf-8" ?>
<beanio xmlns="http://www.beanio.org/2016/07">
  <!-- Ist nicht mehr in Gebrauch und steht im Konflikt zur Version 02, weshalb auch in DSKK-v01 der DBGZ-v01 verwendet wird. -->
  <template name="DBGZ-v01">
    <field name="KE" length="4" literal="DBGZ" required="true"/>
    <field name="KENNZST" length="1" required="true" typeHandler="jn"/>
    <field name="KENNZGLZ" length="1" required="true" regex="[1-4]" type="int" format="0"/>
    <field name="GLEG" length="6" required="true" type="int" format="D6"/>
    <field name="GLEGOEZ" length="6" required="true" type="int" format="D6"/>
    <field name="GLRV" length="6" required="true" type="int" format="D6"/>
    <field name="ZRBG" length="8" required="true" format="yyyyMMdd"/>
    <field name="ZREN" length="8" required="true" format="yyyyMMdd"/>
    <field name="KVAG" length="6" required="true" type="int" format="D6"/>
    <field name="KVAN" length="6" required="true" type="int" format="D6"/>
    <field name="KVSO" length="6" required="true" type="int" format="D6"/>
    <field name="PVAG" length="6" required="true" type="int" format="D6"/>
    <field name="PVAN" length="6" required="true" type="int" format="D6"/>
    <field name="RVAG" length="6" required="true" type="int" format="D6"/>
    <field name="RVAN" length="6" required="true" type="int" format="D6"/>
    <field name="AVAG" length="6" required="true" type="int" format="D6"/>
    <field name="AVAN" length="6" required="true" type="int" format="D6"/>
    <field name="KENNZRK" regex="[WO]" length="1" required="true"/>
  </template>
</beanio>